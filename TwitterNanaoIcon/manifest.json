{
    "name": "Twitter Nanao Icon",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Twitterのタイムライン上のアイコンをななおさんのアイコンにする",
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "content_scripts": [{
      "matches": ["https://twitter.com/*"],
      "css": [
        "content.css"
      ]
    }],
    "web_accessible_resources": [
      "nanao_icon.jpg"
    ]
  }